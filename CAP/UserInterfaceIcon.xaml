<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="CAP.UserInterfaceIcon"
	d:DesignWidth="139" d:DesignHeight="83">

	<Grid x:Name="LayoutRoot">
		<Border BorderThickness="2" Background="#FF858585">
			<Border.BorderBrush>
				<LinearGradientBrush EndPoint="0.774,1.005" StartPoint="0.237,-0.009">
					<GradientStop Color="#FF494646" Offset="1"/>
					<GradientStop Color="#FFB4B4B4"/>
				</LinearGradientBrush>
			</Border.BorderBrush>
			<Canvas Margin="1,-1,-1,1">
				<Rectangle Fill="#FFF4F4F5" Height="18" Stroke="#FF00238F" Width="79" Canvas.Left="50" Canvas.Top="11"/>
				<Rectangle Fill="#FFF4F4F5" Height="18" Stroke="#FF00238F" Canvas.Top="33" Width="79" Canvas.Left="50"/>
				<Rectangle Fill="Black" Height="5" Canvas.Left="8" Canvas.Top="17" Width="38"/>
				<Rectangle Fill="Black" Height="5" Canvas.Left="8" Canvas.Top="40" Width="38"/>
			</Canvas>
		</Border>
	</Grid>
</UserControl>